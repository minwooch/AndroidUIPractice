<?xml version="1.0" encoding="utf-8"?>
<com.applsh.androiduipractice.base.LoadingPlaceHolderConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="96dp">

    <View
        android:id="@+id/daangn_detail_user_placeholder_view"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="48dp"
        android:layout_height="12dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:background="@color/black"
        app:layout_constraintStart_toEndOf="@id/daangn_detail_user_placeholder_view"
        app:layout_constraintTop_toTopOf="@id/daangn_detail_user_placeholder_view" />

    <View
        android:layout_width="72dp"
        android:layout_height="12dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="4dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/daangn_detail_user_placeholder_view"
        app:layout_constraintStart_toEndOf="@id/daangn_detail_user_placeholder_view" />


</com.applsh.androiduipractice.base.LoadingPlaceHolderConstraintLayout>